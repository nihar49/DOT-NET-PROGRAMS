using System;
using System.Collections.Generic;
using System.Linq;
//Read only region start
public class UserMainCode
{
    public int uniqueDigitsCount(int input1)
    {
        //Read only region end
        //Write code here
        
		string str=Convert.ToString(input1);
		string str1="";
		
		foreach(char c in str)
		{
			if(str1.IndexOf(c)==-1)
			{
				str1=str1+c;
			}
		}
		
		int len=str1.Length;
		return len;
    }
}
