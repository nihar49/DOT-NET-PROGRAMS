using System;
using System.Collections.Generic;

//Read only region start
public class UserMainCode
{
    public int countEvensOdds(int input1,int input2,int input3,int input4,int input5,string input6)
    {
        //Read only region end
        int[] arr={input1,input2,input3,input4,input5};
		int count=0;
		if(input6=="odd")
		{
			for(int i=0;i<arr.Length;i++)
			{
				if(arr[i]%2!=0)
				{
					count++;
				}
			}
		}
		if(input6=="even")
		{
			for(int i=0;i<arr.Length;i++)
			{
				if(arr[i]%2==0)
				{
					count++;
				}
			}
		}
		
		return count;
		//Write code here
        throw new NotImplementedException("Method  countEvensOdds(int input1,int input2,int input3,int input4,int input5,string input6) not Implemented.");
    }
}
